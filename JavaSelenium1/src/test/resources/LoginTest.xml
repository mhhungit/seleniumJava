<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Default Suite">
    <!-- Add listeners to generate HTML report -->
    <listeners>
        <listener class-name="org.testng.reporters.TestHTMLReporter"/>
    </listeners>

    <test thread-count="5" name="JavaSelenium">
        <classes>
            <class name="TestAsd">
                <methods>
                    <include name="Test_01"/>
                </methods>
            </class> <!-- TestAsd -->
        </classes>
    </test> <!-- JavaSelenium -->
</suite> <!-- Default Suite -->
